<mapper namespace="com.my.customer.CustomerMapper">
  <select id="selectById" parameterType="java.lang.String"  resultType="com.my.customer.dto.Customer">
    SELECT * FROM customer WHERE id=#{value}
  </select>
  <insert id="insert" parameterType="Customer">
  INSERT INTO customer(id, pwd, name)
  VALUES (#{id}, #{pwd}, #{name})
  </insert>
