package com.my.jpa.entity;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="p_tbl")
/**
 * 상품
 */
public class P {
	@Id
	@Column(name="p_no")
	private String pNo;// 상품번호

	@Column(name="p_name")
	private String pName; //상품명

}
